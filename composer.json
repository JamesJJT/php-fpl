{
    "name": "jamesjjt/php-fpl",
    "description": "PHP client to interact with the FPL API",
    "type": "library",
    "require": {
        "symfony/http-client": "^7.2"
    },
    "require-dev": {
        "pestphp/pest": "^3.7",
        "phpstan/phpstan": "^2.0",
        "laravel/pint": "^1.18",
        "mockery/mockery": "^1.6",
        "symfony/var-dumper": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Jamesjjt\\PhpFpl\\": "src/"
        }
    },
    "authors": [
        {
            "name": "JamesJJT",
            "email": "jastevens2222@hotmail.co.uk"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
